{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
   "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["esnext"],
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"]
    }
  },
  "exclude": ["node_modules"]
}
